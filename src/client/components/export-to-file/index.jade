//- 
    parameters: 
        col-names="comma, separated, column, names"
        filename="generated file's filename"
        tabledata="tableview formatted data (see data-table component)"

+component('export-to-file')
    a(
        class="download-link"
        href="data:attachment/csv,{{ fileContent }}"
        target="_blank"
        download="{{ filename }}"
        on-click="prepareDownload"
        )
        | {{yield}}
