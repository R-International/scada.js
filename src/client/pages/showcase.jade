extends ../templates/template.jade

block title
    title Show Case
block script
    script(src="showcase.js")

block ractive
    inspina-theme
        inspina-menu
        inspina-right
            inspina-header
                span Naber MahmutACCan
            
            inspina-content
                h1 Welcome12 in INSPINIA Static SeedProject

                search-combobox(data="{{productList}}" selected="{{x}}")
                p selected: {{x}}

                search-combobox(db="{{ db }}" view="customers/getReceipts" selected="{{y}}")
                p selected: {{y}}

                flot-chart(data1="{{data1}}" data2 ="{{data2}}")

                div
                    | {{#each [1,2,3]:i}}
                    div.box
                        pie-chart(names="Kekik,Peynir,Zeytin,Mahmut" values="{{simulateData(i)}}")
                    | {{/each}}


            inspina-footer
                +partial('mahmut')
                    span Selam ben mahmut
                    
                +partial('left')
                    span Selam ben left
